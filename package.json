{
  "name": "@maikers/mainframe-sdk",
  "version": "1.0.0",
  "type": "module",
  "description": "TypeScript SDK for Mainframe protocol - Powering the agentic economy through Agent-NFTs with enterprise-grade security, performance optimization, and built-in revenue sharing on Solana",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/browser/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "browser": "./dist/browser/index.js"
    },
    "./integrations": {
      "types": "./dist/types/integrations/index.d.ts",
      "import": "./dist/esm/integrations/index.js",
      "require": "./dist/cjs/integrations/index.js"
    },
    "./integrations/anchor": {
      "types": "./dist/types/integrations/anchor.d.ts",
      "import": "./dist/esm/integrations/anchor.js",
      "require": "./dist/cjs/integrations/anchor.js"
    },
    "./integrations/wallet-adapters": {
      "types": "./dist/types/integrations/wallet-adapters.d.ts",
      "import": "./dist/esm/integrations/wallet-adapters.js",
      "require": "./dist/cjs/integrations/wallet-adapters.js"
    },
    "./testing": {
      "types": "./dist/types/testing/index.d.ts",
      "import": "./dist/esm/testing/index.js",
      "require": "./dist/cjs/testing/index.js"
    }
  },
  "files": [
    "dist/",
    "src/",
    "src/idl/",
    "src/types/mainframe.ts",
    "README.md"
  ],
  "scripts": {
    "sync-program": "cd ../maikers-mainframe && anchor build && cp target/idl/mainframe.json ../maikers-mainframe-sdk/src/idl/ && cp target/types/mainframe.ts ../maikers-mainframe-sdk/src/types/",
    "build": "pnpm run sync-program && pnpm run clean && pnpm run build:types && pnpm run build:cjs && pnpm run build:esm",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "build:browser": "rollup -c rollup.config.js",
    "clean": "rm -rf dist/",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:production": "jest --config=jest.production.config.js",
    "test:security": "jest --testPathPattern=security",
    "test:performance": "jest --testPathPattern=performance",
    "test:coverage": "jest --coverage --coverageThreshold='{\n  \"global\": {\n    \"branches\": 90,\n    \"functions\": 90,\n    \"lines\": 90,\n    \"statements\": 90\n  }\n}'",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "security-audit": "pnpm audit && pnpm dlx audit-ci --config .audit-ci.json",
    "docs": "typedoc",
    "docs:build": "typedoc --out docs/",
    "health-check": "node -p 'require(\"./dist/cjs/index.js\").globalResourceMonitor.runHealthChecks()'",
    "performance-check": "node scripts/performance-check.js",
    "verify-fee-accounts": "node scripts/verify-fee-accounts.js",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "version:patch": "pnpm version patch --no-git-tag-version && pnpm run changelog",
    "version:minor": "pnpm version minor --no-git-tag-version && pnpm run changelog",
    "version:major": "pnpm version major --no-git-tag-version && pnpm run changelog",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch",
    "release:pre": "standard-version --prerelease",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "check-version": "node scripts/check-version.js",
    "release-script": "node scripts/release.js",
    "release-patch": "node scripts/release.js patch",
    "release-minor": "node scripts/release.js minor", 
    "release-major": "node scripts/release.js major",
    "release-pre": "node scripts/release.js prerelease",
    "postversion": "git push && git push --tags && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@metaplex-foundation/mpl-core": "^1.7.0",
    "@metaplex-foundation/umi": "^0.9.2",
    "@metaplex-foundation/umi-bundle-defaults": "^0.9.2",
    "@metaplex-foundation/umi-uploader-irys": "^0.9.2",
    "@solana/spl-token": "^0.4.14",
    "@solana/wallet-adapter-backpack": "^0.1.14",
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/wallet-adapter-glow": "^0.1.18",
    "@solana/wallet-adapter-phantom": "^0.9.28",
    "@solana/wallet-adapter-solflare": "^0.6.32",
    "@solana/web3.js": "^1.98.4",
    "arweave": "^1.15.7",
    "bs58": "^5.0.0",
    "ipfs-http-client": "^60.0.1",
    "libsodium-wrappers-sumo": "^0.7.15",
    "node-fetch": "^3.3.2",
    "sharp": "^0.33.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.14",
    "@types/libsodium-wrappers-sumo": "^0.7.8",
    "@types/node": "^20.19.22",
    "@types/node-fetch": "^2.6.13",
    "@types/sharp": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "audit-ci": "^7.1.0",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-commits-detector": "^1.0.3",
    "conventional-recommended-bump": "^9.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-security": "^2.1.1",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "rollup": "^4.52.5",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.4.5",
    "typedoc": "^0.25.13",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@solana/wallet-adapter-react": ">=0.15.0",
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "@solana/wallet-adapter-react": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MaikersHQ/maikers-mainframe-sdk.git"
  },
  "keywords": [
    "solana",
    "anchor",
    "ai-agents",
    "elizaos",
    "blockchain",
    "defi",
    "sdk",
    "encryption",
    "ipfs",
    "arweave",
    "nft",
    "typescript",
    "production-ready",
    "enterprise",
    "security",
    "performance",
    "monitoring",
    "reliability",
    "rate-limiting",
    "circuit-breaker",
    "audit-logging",
    "metaplex-core",
    "zero-knowledge"
  ],
  "author": "Maikers",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/MaikersHQ/maikers-mainframe-sdk/issues"
  },
  "homepage": "https://github.com/MaikersHQ/maikers-mainframe-sdk#readme"
}